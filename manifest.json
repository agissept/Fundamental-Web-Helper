{
    "name": "Fundamental Web Helper",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts":[{
        "matches": ["file:///*", "https://www.dicoding.com/*"],
        "js": ["dropdown.js", "foreground.js"]
    }],
    "background": {
        "scripts": ["background.js"],
        "persistent": false

    },
    "browser_action": {},
    "permissions": ["tabs",
        "contextMenus",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
